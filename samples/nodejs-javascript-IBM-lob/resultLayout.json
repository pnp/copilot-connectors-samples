{
  "type": "AdaptiveCard",
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.3",
  "body": [
      {
          "type": "Container",
          "items": [
              {
                  "type": "ColumnSet",
                  "columns": [
                      {
                          "type": "Column",
                          "width": "auto",
                          "items": [
                              {
                                  "type": "Image",
                                  "url": "https://cdn-icons-png.flaticon.com/512/12575/12575556.png",
                                  "width": "32px",
                                  "height": "32px"
                              }
                          ]
                      },
                      {
                          "type": "Column",
                          "width": "stretch",
                          "items": [
                              {
                                  "type": "TextBlock",
                                  "text": "Client Orders",
                                  "wrap": true,
                                  "color": "Accent",
                                  "size": "Large",
                                  "horizontalAlignment": "Left"
                              }
                          ],
                          "verticalContentAlignment": "Center",
                          "height": "stretch"
                      }
                  ]
              },
              {
                  "type": "Container",
                  "spacing": "Medium",
                  "items": [
                      {
                          "type": "ColumnSet",
                          "columns": [
                              {
                                  "type": "Column",
                                  "width": "auto",
                                  "items": [
                                      {
                                          "type": "TextBlock",
                                          "text": "Customer Id :",
                                          "wrap": true,
                                          "isSubtle": true,
                                          "color": "Accent",
                                          "horizontalAlignment": "Left",
                                          "weight": "Bolder"
                                      },
                                      {
                                          "type": "TextBlock",
                                          "text": " Name :",
                                          "wrap": true,
                                          "isSubtle": true,
                                          "color": "Accent",
                                          "horizontalAlignment": "Left",
                                          "weight": "Bolder",
                                          "separator": true,
                                          "spacing": "Medium"
                                      },
                                      {
                                          "type": "TextBlock",
                                          "text": "Email :",
                                          "isSubtle": true,
                                          "color": "Accent",
                                          "maxLines": 1,
                                          "horizontalAlignment": "Left",
                                          "weight": "Bolder",
                                          "separator": true,
                                          "spacing": "Medium"
                                      },
                                      {
                                          "type": "TextBlock",
                                          "text": "Country / City :",
                                          "wrap": true,
                                          "isSubtle": true,
                                          "color": "Accent",
                                          "weight": "Bolder",
                                          "separator": true,
                                          "spacing": "Medium"
                                      }
                                  ]
                              },
                              {
                                  "type": "Column",
                                  "width": "stretch",
                                  "items": [
                                      {
                                          "type": "TextBlock",
                                          "text": "${custcode}",
                                          "wrap": true,
                                          "horizontalAlignment": "Left"
                                      },
                                      {
                                          "type": "TextBlock",
                                          "text": "${custname}",
                                          "wrap": true,
                                          "horizontalAlignment": "Left",
                                          "separator": true,
                                          "spacing": "Medium"
                                      },
                                      {
                                          "type": "TextBlock",
                                          "text": "${email}",
                                          "wrap": true,
                                          "horizontalAlignment": "Left",
                                          "separator": true,
                                          "spacing": "Medium"
                                      },
                                      {
                                          "type": "TextBlock",
                                          "text": "${concat(state, \", \", country)}",
                                          "wrap": true,
                                          "horizontalAlignment": "Left",
                                          "separator": true,
                                          "spacing": "Medium"
                                      }
                                  ],
                                  "horizontalAlignment": "Left",
                                  "verticalContentAlignment": "Top",
                                  "minHeight": "32px",
                                  "height": "stretch"
                              }
                          ]
                      }
                  ],
                  "style": "default"
              },
              {
                  "type": "Container",
                  "items": [
                      {
                          "type": "ColumnSet",
                          "columns": [
                              {
                                  "type": "Column",
                                  "width": "auto",
                                  "items": [
                                      {
                                          "type": "Image",
                                          "url": "https://t3.ftcdn.net/jpg/06/19/46/46/240_F_619464663_PWLJHM0mxaKdZFYNQhfPFNAaWMtb69bL.jpg",
                                          "width": "32px",
                                          "height": "32px",
                                          "size": "Small"
                                      }
                                  ]
                              },
                              {
                                  "type": "Column",
                                  "width": "stretch",
                                  "items": [
                                      {
                                          "type": "TextBlock",
                                          "text": "Latest Orders ",
                                          "wrap": true,
                                          "size": "Medium",
                                          "weight": "Default",
                                          "color": "Accent",
                                          "isSubtle": false
                                      }
                                  ],
                                  "verticalContentAlignment": "Center"
                              }
                          ],
                          "spacing": "Large"
                      },
                      {
                          "type": "Container",
                          "items": [
                              {
                                  "type": "ColumnSet",
                                  "columns": [
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "Order number",
                                                  "wrap": true,
                                                  "isSubtle": true,
                                                  "height": "stretch",
                                                  "color": "Accent",
                                                  "weight": "Bolder"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "Order Date",
                                                  "wrap": true,
                                                  "isSubtle": true,
                                                  "weight": "Bolder",
                                                  "color": "Accent"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "Total Cost",
                                                  "wrap": true,
                                                  "isSubtle": true,
                                                  "weight": "Bolder",
                                                  "color": "Accent"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "Status ",
                                                  "wrap": true,
                                                  "isSubtle": true,
                                                  "weight": "Bolder",
                                                  "color": "Accent"
                                              }
                                          ]
                                      }
                                  ]
                              },
                              {
                                  "type": "ColumnSet",
                                  "columns": [
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orders[0]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(orders), 1)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orderdates[0]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(orderdates), 1)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${ordertotals[0]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(ordertotals), 1)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": " ${orderstatus[0]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Good",
                                                  "size": "Default",
                                                  "$when": "${greaterOrEquals(count(orderstatus), 1)}"
                                              }
                                          ]
                                      }
                                  ]
                              },
                              {
                                  "type": "ColumnSet",
                                  "columns": [
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orders[1]}",
                                                  "wrap": true,
                                                  "$when": "${greaterOrEquals(count(orders), 2)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orderdates[1]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(orderdates), 2)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${ordertotals[1]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(ordertotals), 2)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": " ${orderstatus[1]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Good",
                                                  "size": "Default",
                                                  "$when": "${greaterOrEquals(count(orderstatus), 2)}"
                                              }
                                          ]
                                      }
                                  ]
                              },
                               {
                                  "type": "ColumnSet",
                                  "columns": [
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orders[2]}",
                                                  "wrap": true,
                                                  "$when": "${greaterOrEquals(count(orders), 3)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orderdates[2]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(orderdates), 3)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${ordertotals[2]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(ordertotals), 3)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": " ${orderstatus[2]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Good",
                                                  "size": "Default",
                                                  "$when": "${greaterOrEquals(count(orderstatus), 3)}"
                                              }
                                          ]
                                      }
                                  ]
                              },
                              {
                                  "type": "ColumnSet",
                                  "columns": [
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orders[3]}",
                                                  "wrap": true,
                                                  "$when": "${greaterOrEquals(count(orders), 4)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orderdates[3]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(orderdates), 4)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${ordertotals[3]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(ordertotals),4)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": " ${orderstatus[3]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Good",
                                                  "size": "Default",
                                                  "$when": "${greaterOrEquals(count(orderstatus), 4)}"
                                              }
                                          ]
                                      }
                                  ]
                              },
                              {
                                  "type": "ColumnSet",
                                  "columns": [
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orders[4]}",
                                                  "wrap": true,
                                                  "$when": "${greaterOrEquals(count(orders), 5)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${orderdates[4]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(orderdates), 5)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${ordertotals[4]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Default",
                                                  "$when": "${greaterOrEquals(count(ordertotals),5)}"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": " ${orderstatus[4]}",
                                                  "wrap": true,
                                                  "isSubtle": false,
                                                  "color": "Good",
                                                  "size": "Default",
                                                  "$when": "${greaterOrEquals(count(orderstatus), 5)}"
                                              }
                                          ]
                                      }
                                  ]
                              }
                          ],
                          "style": "default"
                      }
                  ],
                  "spacing": "Medium"
              }
          ],
          "style": "emphasis"
      }
  ]
}
